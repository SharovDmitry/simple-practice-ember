<div class="col-sm-12 mb-4">
  <div class="card">
    <div class="card-body">
      <div class="d-flex justify-content-between align-items-center">
        <div>
          <h5>{{@service.description}}</h5>
          <div class="text-muted">{{@service.duration}} minutes</div>
        </div>
        {{#link-to "offices" @service.id invokeAction=(action "select" @service) class="btn btn-primary"}}
          Select
        {{/link-to}}
      </div>
    </div>
  </div>
</div>
